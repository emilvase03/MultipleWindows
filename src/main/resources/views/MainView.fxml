<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="410.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.multiplewindows.controllers.MainWindowController">
   <children>
       <Button layoutX="47.0" layoutY="196.0" onAction="#onWindowOneButtonClicked" text="Open Window 1">
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </Button>
      <Button layoutX="46.0" layoutY="286.0" mnemonicParsing="false" onAction="#onWindowTwoButtonClicked" text="Open Window 2">
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </Button>
      <TextField fx:id="nameTxt" layoutX="25.0" layoutY="111.0" />
      <Label layoutX="37.0" layoutY="78.0" text="Data to bring to Window 1" />
      <TextField fx:id="dataFromWindow2Txt" disable="true" editable="false" layoutX="277.0" layoutY="243.0" />
      <Label layoutX="302.0" layoutY="211.0" text="Data from Window 2" />
   </children>
</AnchorPane>
